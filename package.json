{
  "name": "quake-terminal",
  "description": "Quickly launch a terminal in Quake mode using a keyboard shortcut",
  "version": "1.6.9",
  "private": true,
  "repository": "github:diegodario88/quake-terminal",
  "author": "Diego Dario",
  "license": "LGPL-3.0-or-later",
  "type": "module",
  "bugs": {
    "url": "https://github.com/diegodario88/quake-terminal/issues"
  },
  "engines": {
    "node": ">=22"
  },
  "homepage": "https://github.com/diegodario88/quake-terminal#readme",
  "sideEffects": false,
  "devDependencies": {
    "@girs/gnome-shell": "^48.0.2",
    "@types/node": "^22.13.14",
    "esbuild": "^0.25.9",
    "esbuild-plugin-copy": "^2.1.1",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsdoc": "^50.6.17",
    "prettier": "^3.6.2"
  },
  "config": {
    "uuid": "quake-terminal@diegodario88.github.io"
  },
  "scripts": {
    "clean": "rm -rf ./dist ./*.shell-extension.zip",
    "build": "node ./build.js",
    "extension:install": "npm run build && gnome-extensions install -f ./${npm_package_config_uuid}.shell-extension.zip",
    "extension:uninstall": "gnome-extensions uninstall ${npm_package_config_uuid}"
  }
}
